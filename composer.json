{
    "name": "localelogic/api",
    "type": "project",
    "description": "LocaleLogic API - UK Property Geography Service",
    "keywords": ["laravel", "api", "geography", "uprn"],
    "license": "proprietary",
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.fluxui.dev"
        }
    ],
    "require": {
        "php": "^8.2",
        "halaxa/json-machine": "^1.2",
        "laravel/boost": "^1.8",
        "laravel/breeze": "^2.0",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "league/csv": "^9.28",
        "livewire/flux-pro": "^2.10",
        "livewire/livewire": "^3.7",
        "proj4php/proj4php": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
